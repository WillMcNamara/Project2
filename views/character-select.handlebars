<h3>Select Your Character</h3>
<div class="row">
    <div class="col-12">
        <button class="btn btn-primary">Create New Character</button>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        {{#each character}}
            <div class="row">
                <div class="card">
                    <h4 class="card-title">{{name}}</h4>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-4">
                                //image
                            </div>
                            <div class="col-8">
                                <h3>Skills</h3>
                                <ul>
                                    <li>{{skill_F}}</li>
                                    <li>{{skill_J}}</li>
                                    <li>{{skill_T}}</li>
                                    <li>{{skill_C}}</li>
                                    <li>{{skill_I}}</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>